{
  "name": "Notionize",
  "description": "A tool to easily and quickly add YouTube videos, articles, comments, or other online documents to a Notion database",
  "author": "Sauharda Rajbhandari",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "src/popup.html",
    "default_icon": {
      "16": "assets/logo1.png",
      "32": "assets/logo2.png",
      "48": "assets/logo3.png",
      "128": "assets/logo3.png"
    }
  },
  "icons": {
    "16": "assets/logo1.png",
    "32": "assets/logo2.png",
    "48": "assets/logo3.png",
    "128": "assets/logo3.png"
  },
  "background": {
    "scripts": [
      "src/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/content.js"
      ],
      "run_at": "document_end"
    }
  ]
}