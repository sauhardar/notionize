{
  "name": "Notionize",
  "description": "A Google Chrome extension that makes adding a memorable YouTube video to your Notion database quick and effortless.",
  "author": "Sauharda Rajbhandari",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "src/popup.html",
    "default_icon": {
      "16": "assets/logo1.png",
      "32": "assets/logo2.png",
      "48": "assets/logo3.png",
      "128": "assets/logo3.png"
    }
  },
  "icons": {
    "16": "assets/logo1.png",
    "32": "assets/logo2.png",
    "48": "assets/logo3.png",
    "128": "assets/logo3.png"
  },
  "background": {
    "scripts": [
      "src/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "tabs",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "content_security_policy": "script-src 'self' https://unpkg.com/axios/dist/axios.min.js; object-src 'self'"
}